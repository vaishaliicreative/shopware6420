<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="ICTShopFinder\Core\Content\ShopFinder\Aggregate\ShopFinderTranslation\ShopFinderTranslationDefinition">
            <tag name="shopware.entity.definition" entity="ict_shop_finder_translation"/>
        </service>

        <service id="ICTShopFinder\Core\Content\ShopFinder\ShopFinderDefinition">
            <tag name="shopware.entity.definition" entity="ict_shop_finder" />
        </service>

        <service id="ICTShopFinder\Core\Content\Extension\Country\CountryExtension" >
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="ICTShopFinder\Core\Content\Extension\Language\LanguageExtension">
            <tag name="shopware.entity.extension"/>
        </service>
        <service id="ICTShopFinder\Core\Api\DemoDataController" public="true">
            <argument id="country.repository" type="service"/>
            <argument id="ict_shop_finder.repository" type="service" />
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="ICTShopFinder\Storefront\Subscriber\FooterSubscriber">
            <argument id="Shopware\Core\System\SystemConfig\SystemConfigService" type="service"/>
            <argument id="ict_shop_finder.repository" type="service" />
            <tag name="kernel.event_subscriber" />
        </service>

        <service id="ICTShopFinder\Resources\snippet\en_GB\SnippetFile_en_GB">
            <tag name="shopware.snippet.file"/>
        </service>
    </services>
</container>
