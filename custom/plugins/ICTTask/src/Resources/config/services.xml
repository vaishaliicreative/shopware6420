<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="ICTTask\Core\Content\IctTask\Aggregate\IctTaskTranslation\IctTaskTranslationDefinition">
            <tag name="shopware.entity.definition" entity="ict_task_translation"/>
        </service>

        <service id="ICTTask\Core\Content\IctTask\IctTaskDefinition">
            <tag name="shopware.entity.definition" entity="ict_task"/>
        </service>

        <service id="ICTTask\Core\Content\Extension\Country\CountryExtension">
            <tag name="shopware.entity.extension" />
        </service>

        <service id="ICTTask\Core\Content\Extension\State\StateExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="ICTTask\Core\Content\Extension\Language\LanguageExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="ICTTask\Core\Content\Extension\Media\MediaExtension">
            <tag name="shopware.entity.extension" />
        </service>

        <service id="ICTTask\Core\Content\Extension\Product\ProductExtension">
            <tag name="shopware.entity.extension"/>
        </service>
    </services>
</container>
