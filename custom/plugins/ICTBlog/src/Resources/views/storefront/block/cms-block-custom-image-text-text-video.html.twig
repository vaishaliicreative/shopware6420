{% block block_custom_image_text_text_video %}
    {% set columns = 2 %}
    {% set id = element.id %}

    {% block block_custom_image_text_text_video_first_image %}
        {% set element = block.slots.getSlot('firstImage') %}
        <div class="col-md-6" data-cms-element-id="{{ element.id }}">
            {% block block_custom_image_text_text_video_first_image_inner %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    {% endblock %}

    {% block block_custom_image_text_text_video_first_text %}
        {% set element = block.slots.getSlot('firstText') %}
        <div class="col-md-6" data-cms-element-id="{{ element.id }}">
            {% block block_custom_image_text_text_video_first_text_inner %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    {% endblock %}

    {% block block_custom_image_text_text_video_second_text %}
        {% set element = block.slots.getSlot('secondText') %}
        <div class="col-md-6" data-cms-element-id="{{ element.id }}">
            {% block block_custom_image_text_text_video_second_text_inner %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    {% endblock %}

    {% block block_custom_image_text_text_video_video_content %}
        {% set element = block.slots.getSlot('videoContent') %}
        <div class="col-md-6" data-cms-element-id="{{ element.id }}">
            {% block block_custom_image_text_text_video_video_content_inner %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    {% endblock %}

{% endblock %}
