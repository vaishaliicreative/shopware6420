{% block block_custom_image_text_video %}
    {% set columns = 3 %}
    {# @deprecated tag:v6.5.0 - Use "element.id" directly instead. #}
    {% set id = element.id %}

    {% block block_custom_image_text_video_left %}
        {% set element = block.slots.getSlot('left-image') %}

        <div class="col-md-4" data-cms-element-id="{{ element.id }}">
            {% block block_custom_image_text_video_left_inner %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    {% endblock %}

    {% block block_custom_image_text_video_center %}
        {% set element = block.slots.getSlot('center-text') %}

        <div class="col-md-4" data-cms-element-id="{{ element.id }}">
            {% block block_custom_image_text_video_center_inner %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    {% endblock %}

    {% block block_custom_image_text_video_right %}
        {% set element = block.slots.getSlot('right-video') %}

        <div class="col-md-4" data-cms-element-id="{{ element.id }}">
            {% block block_custom_image_text_video_right_inner %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    {% endblock %}
{% endblock %}
